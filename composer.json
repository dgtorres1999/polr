{
    "name": "cydrobolt/polr",
    "description": "A Modern, Powerful, Open Source Link Shortener",
    "keywords": ["polr", "url-shortener", "shorten", "laravel", "lumen"],
    "license": "GPL-3.0+",
    "require": {
        "php": "^8.0",
        "laravel/lumen-framework": "^8.0",
        "vlucas/phpdotenv": "^5.0",
        "illuminate/mail": "~5.1",
        "yajra/laravel-datatables-oracle": "^10.0",
        "paragonie/random_compat": "^1.0.6",
        "torann/geoip": "^1.0",
        "geoip2/geoip2": "^2.4",
        "nesbot/carbon": "^2.0",
        "doctrine/dbal": "^3.0",
        "google/recaptcha": "~1.1",
        "symfony/http-foundation": "2.7.51"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.setup', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}